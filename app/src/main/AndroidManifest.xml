<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.TMA"
        tools:targetApi="31">

        <activity
            android:name="com.example.tma.RegisterScreenActivity"
            android:exported="false"
            android:theme="@style/Theme.LoginPage" />

        <activity
            android:name="com.example.tma.LoginScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAi5UyU8HRYbPO1LWXgKFRnCxvP7T30hQI" />
        <activity
            android:name="com.example.tma.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.TMA" />
    </application>

</manifest>